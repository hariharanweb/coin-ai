{
  "name": "coin-ai",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "server": "node ./dist-server/app",
    "dev": "NODE_ENV=development npm-run-all build server",
    "prod": "NODE_ENV=production npm-run-all build server",
    "transpile": "babel ./source --out-dir dist-server",
    "clean": "rimraf dist-server",
    "watch:dev": "nodemon",
    "alerter": "node -r dotenv/config ./dist-server/alerter/alerter"
  },
  "dependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "axios": "^0.24.0",
    "body-parser": "^1.19.1",
    "cookie-parser": "~1.4.4",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "source/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/transform-runtime"
      ]
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.16.7",
    "babel-plugin-transform-runtime": "^6.23.0"
  }
}
